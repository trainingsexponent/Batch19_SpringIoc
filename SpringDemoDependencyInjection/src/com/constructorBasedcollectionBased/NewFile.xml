<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"https://www.springframework.org/dtd/spring-beans-2.0.dtd">

	
	<beans>
	  <bean id="s" class="com.constructorBasedcollectionBased.Shopkeeper">
	     <constructor-arg value="123"></constructor-arg>
	     <constructor-arg value="D-mart"></constructor-arg>
	     <constructor-arg>
	       <list>
	         <value>Laptop</value>
	         <value>Mobile</value>
	       </list>
	     </constructor-arg>
	     <constructor-arg>
	       <set>
	         <value>Jeans</value>
	         <value>T-Shirt</value>
	       </set>
	     </constructor-arg>
	     <constructor-arg>
	       <map>
	         <entry key="1" value="Casual Shoes"></entry>
	         <entry key="2" value="Formal Shoes"></entry>
	       </map>
	     </constructor-arg>
	  </bean>
	</beans>